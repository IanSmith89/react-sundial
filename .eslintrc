{
	"env": {
		"node": true
	},
	"extends": ["airbnb", "plugin:prettier/recommended", "prettier/react"],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2020,
		"ecmaFeatures": {
			"legacyDecorators": true,
			"jsx": true
		}
	},
	"plugins": ["react", "react-hooks", "import", "jsx-a11y", "prettier"],
	"rules": {},
	"settings": {
		"import/resolver": {
			"node": {
				"extensions": [".js", ".jsx"]
			}
		},
		"react": {
			"version": "16"
		}
	}
}
